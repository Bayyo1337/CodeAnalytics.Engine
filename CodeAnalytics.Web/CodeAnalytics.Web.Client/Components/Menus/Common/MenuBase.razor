
@using CodeAnalytics.Web.Client.Menus
@using CodeAnalytics.Web.Client.Menus.Interfaces

@typeparam TResult where TResult : class
@implements IMenuComponent<TResult>

@inject MenuService MenuService

@code {
   
   [Parameter]
   public int MenuIndex { get; set; }
   
   [Parameter]
   public ulong MenuId { get; set; }

   public void SetResult(TResult? result)
   {
      MenuService.SetResult(MenuId, result);
   }
}